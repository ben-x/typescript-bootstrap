{
  "name": "typescript-bootstrap",
  "version": "1.0.0",
  "private": true,
  "description": "Bootstrapping typescript application for nodejs with express",
  "repository": "https://github.com/ben-x/typescript-bootstrap.git",
  "main": "src/index.ts",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "node_modules/.bin/nodemon ./dist/index.js",
    "lint": "eslint --ext .ts,.json --max-warnings=0 src",
    "build": "rm -rf ./dist && npx tsc",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json"
  },
  "keywords": [
    "typescript",
    "jsdoc",
    "express",
    "eslint",
    "bootstrap"
  ],
  "author": "benx",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "hbs": "^4.1.1",
    "helmet": "^3.22.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.15",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.31",
    "@types/express": "^4.17.6",
    "@types/express-validator": "^3.0.0",
    "@types/mocha": "^7.0.2",
    "@types/mongoose": "^5.7.20",
    "@types/node": "^14.0.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "better-docs": "^2.0.1",
    "copy-files": "^0.1.0",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^3.6.4",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "engines": {
    "node": ">=12.15.0",
    "npm": "^6.13.4"
  }
}
